pan class="line">      this.getChartData();</span><br><span class="line">      window.addEventListener(&quot;resize&quot;, () =&gt; {</span><br><span class="line">        this.resize();           //&#x76D1;&#x542C;&#x5C4F;&#x5E55;&#x5927;&#x5C0F;&#xFF0C;&#x6765;&#x5237;&#x65B0;&#x753B;&#x5E03;</span><br><span class="line">      });</span><br><span class="line">    });</span><br><span class="line">  },</span><br><span class="line">  methods: {</span><br><span class="line">    //&#x67E5;&#x8BE2;&#x56FE;&#x8868;&#x6570;&#x636E;  &#x52A8;&#x6001;&#x7684;&#x6539;&#x53D8;&#x6570;&#x636E;</span><br><span class="line">    getChartData() {</span><br><span class="line">      // this.chart.setOption({</span><br><span class="line">      //   xAxis: {</span><br><span class="line">      //     data: this.xAxisdata</span><br><span class="line">      //   },</span><br><span class="line">      //   series: {</span><br><span class="line">      //     data: this.seriesdata</span><br><span class="line">      //   }</span><br><span class="line">      // });</span><br><span class="line">      this.chart.hideLoading();</span><br><span class="line">    },</span><br><span class="line">    //&#x521D;&#x59CB;&#x5316;&#x56FE;&#x8868;</span><br><span class="line">    initChart() {</span><br><span class="line">      let option = {</span><br><span class="line">        title: {</span><br><span class="line">        },</span><br><span class="line">         tooltip: {</span><br><span class="line">          trigger: &quot;axis&quot;,</span><br><span class="line">          formatter: &quot;&#x65F6;&#x95F4; : {b}&lt;br /&gt;&#x538B;&#x529B;&#xFF1A;{c}Mpa&quot;,</span><br><span class="line">          axisPointer: {</span><br><span class="line">            type: &quot;line&quot; // &#x9ED8;&#x8BA4;&#x4E3A;&#x76F4;&#x7EBF;&#xFF0C;&#x53EF;&#x9009;&#x4E3A;&#xFF1A;&apos;line&apos; | &apos;shadow&apos;</span><br><span class="line">          }</span><br><span class="line">        },</span><br><span class="line"